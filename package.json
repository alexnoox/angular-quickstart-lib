{
  "name": "quickstart-lib",
  "version": "1.0.0",
  "description": "Library QuickStart package.json from the documentation, supplemented with testing support",
  "main": "./bundles/quickstart-lib.umd.js",
  "module": "./quickstart-lib.es5.js",
  "es2015": "./quickstart-lib.js",
  "typings": "./quickstart-lib.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "build-demo": "rimraf dist && webpack --progress --profile --bail",
    "build-demo:watch": "rimraf dist && webpack --progress --profile --bail --watch",
    "serve": "webpack-dev-server --inline --progress --port 8080",
    "start": "npm run serve",
    "test": "karma start karma.conf.js --no-single-run --auto-watch",
    "test:once": "karma start karma.conf.js --single-run",
    "preintegration": "npm run build && cd integration && npm run clean && npm install",
    "integration": "npm run integration:aot && npm run integration:jit",
    "integration:jit": "cd integration && npm run e2e",
    "integration:aot": "cd integration && npm run e2e:aot",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "release": "standard-version"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0 <5.0.0 || >=4.0.0-beta <5.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.3.2",
    "@angular/compiler-cli": "^4.3.2",
    "@angular/core": "^4.3.2",
    "@angular/forms": "^4.3.2",
    "@angular/platform-browser": "^4.3.2",
    "@angular/platform-browser-dynamic": "^4.3.2",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.19",
    "@types/selenium-webdriver": "3.0.4",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.2",
    "camelcase": "^4.1.0",
    "codelyzer": "3.1.2",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.1.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.4",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.3",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.3.3",
    "protractor": "^5.1.2",
    "raw-loader": "0.5.1",
    "reflect-metadata": "^0.1.10",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.2",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "systemjs": "^0.20.17",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "3.4.1",
    "webpack-dev-server": "2.6.1",
    "zone.js": "^0.8.16"
  }
}
